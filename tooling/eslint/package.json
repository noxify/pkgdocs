{
  "name": "@pkgdocs/eslint-config",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "./base": "./base.js",
    "./react": "./react.js",
    "./nextjs": "./nextjs.js"
  },
  "scripts": {
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "clean:cache": "git clean -xdf .cache",
    "format": "prettier --check . --ignore-path ../../.gitignore --ignore-path ../../.prettierignore",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@eslint/compat": "2.0.0",
    "@next/eslint-plugin-next": "16.1.1",
    "eslint-config-turbo": "2.7.3",
    "eslint-plugin-import": "2.32.0",
    "eslint-plugin-package-json": "0.88.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "7.0.1",
    "jsonc-eslint-parser": "2.4.2",
    "typescript-eslint": "8.52.0"
  },
  "devDependencies": {
    "@eslint/js": "9.39.2",
    "@pkgdocs/prettier-config": "workspace:*",
    "@pkgdocs/tsconfig": "workspace:*",
    "eslint": "9.39.2",
    "prettier": "3.7.4",
    "typescript": "5.9.3"
  },
  "prettier": "@pkgdocs/prettier-config"
}
